<FindBugsFilter>
    <!-- Accepting generated builders to have unusual namings -->
    <Match>
        <Bug pattern="NM_METHOD_NAMING_CONVENTION"/>
        <Class name="~.*Builder"/>
    </Match>

    <!-- Accepting possible exposure of mutable object in getter -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/>
        <Class name="se.digg.wallet.account.infrastructure.model.AccountEntity"/>
    </Match>

    <!-- Accepting possible exposure of mutable object in constructor or setter -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP2"/>
        <Class name="se.digg.wallet.account.infrastructure.model.AccountEntity"/>
    </Match>
</FindBugsFilter>
